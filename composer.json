{
    "name": "qase-tms/qase-php-api",
    "description": "Qase REST API Client for PHP Users.",
    "type": "library",
    "keywords": [
        "qase",
        "qase-php",
        "rest",
        "qase-rest"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "ekorniushin",
            "email": "paladinn.jeka@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "netresearch/jsonmapper": "^2.0",
        "vlucas/phpdotenv": "^4.0|^5.0",
        "psr/http-client": "^1.0",
        "php-http/guzzle7-adapter": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7 <6",
        "mockery/mockery": "^0.9.4",
        "symfony/var-dumper": "~2.8|~3.0",
        "phpstan/phpstan": "^0.12.23"
    },
    "autoload": {
        "psr-4": {
            "QaseRestApi\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QaseRestApi\\JiraRestApiServiceProvider"
            ]
        }
    }
}